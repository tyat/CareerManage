<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojo.CmDirection" table="cm_direction" schema="career" dynamic-update="true" dynamic-insert="true">
        <id name="did" type="java.lang.Integer">
            <column name="did" />
            <generator class="native" />
        </id>
        <property name="dname" column="dname"/>
        <property name="dstate" column="dstate"/>
        <set name="cmUnempsByDid" inverse="true" cascade="all">
            <key>
                <column name="did"/>
            </key>
            <one-to-many not-found="ignore" class="com.pojo.CmUnemp"/>
        </set>
    </class>
</hibernate-mapping>